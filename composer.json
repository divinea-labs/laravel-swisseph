{
    "name": "divinea-labs/laravel-swisseph",
    "description": "A Laravel wrapper for the Swiss Ephemeris library, providing accurate astronomical and astrological calculations.",
    "keywords": [
        "divinealabs",
        "laravel",
        "laravel-swisseph"
    ],
    "homepage": "https://github.com/divinea-labs/laravel-swisseph",
    "license": "MIT",
    "authors": [
        {
            "name": "skyel",
            "email": "skyel@divinea.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3 || ^8.4",
        "illuminate/contracts": "^11.0 || ^12.0",
        "spatie/laravel-data": "^4.18",
        "spatie/laravel-package-tools": "^1.16",
        "phpdocumentor/reflection": "^6.1"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.0 || ^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DivineaLabs\\Swisseph\\": "src/",
            "DivineaLabs\\Swisseph\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DivineaLabs\\Swisseph\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DivineaLabs\\Swisseph\\SwissephServiceProvider"
            ],
            "aliases": {
                "Swisseph": "DivineaLabs\\Swisseph\\Facades\\Swisseph"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
